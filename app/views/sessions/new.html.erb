<div>
  <h1>Log In To FinFriend</h1>

  <div class="row">
    <div class="col s12"><h4>Welcome Back!</h4></div>
  </div>

  <%= form_for :login do |f| %>
  	<div class="row">
      <div class="input-field col s12">
        <%= f.label :email %>
        <%= f.text_field :email, autofocus: true, :required => true %>
      </div>
    </div>
		<div class="row">
      <div class="input-field col s12">
        <%= f.label :password %>
        <%= f.password_field :password, :required => true %>
      </div>
    </div>
    <div class="input-field col s12">
      <%= render partial: '/submit_button' %>
    </div>
  <% end %>
</div>
